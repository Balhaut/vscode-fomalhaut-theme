    "tokenColors": [
        {
            "settings": {
                "foreground": "#bb9065"
            }
        },
        {
            "name": "HTML: Doctype Declaration",
            "scope": [
                "meta.tag.sgml.doctype",
                "meta.tag.sgml.doctype string",
                "meta.tag.sgml.doctype entity.name.tag",
                "meta.tag.sgml punctuation.definition.tag.html"
            ],
            "settings": {
                "foreground": "#a045e4"
            }
        },
        {
            "name": "HTML: Tags",
            "scope": [
                "meta.tag",
                "punctuation.definition.tag.html",
                "punctuation.definition.tag.begin.html",
                "punctuation.definition.tag.end.html"
            ],
            "settings": {
                "foreground": "#a045e4"
            }
        },
        {
            "name": "HTML: Tag Names",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#b564ea"
            }
        },
        {
            "name": "HTML: Attribute Names",
            "scope": [
                "meta.tag entity.other.attribute-name",
                "entity.other.attribute-name.html"
            ],
            "settings": {
                "foreground": "#e08700"
            }
        },
        {
            "scope": [
                "meta.brace",
                "punctuation.definition.array",
                "punctuation.definition.binding-pattern",
                "punctuation.definition.block",
                "punctuation.definition.dictionary",
                "punctuation.definition.string",
                "punctuation.definition.tag",
                "punctuation.section",
                "punctuation.separator",
                "punctuation.support",
                "punctuation.terminator"
            ],
            "settings": {
                "foreground": "#4b76b4"
            }
        },
        {
            "scope": [
                "punctuation.section.embedded.begin.php",
                "punctuation.section.embedded.end.php"
            ],
            "settings":{
                "foreground": "#4b76b4"
            }
        },
        {
            "scope": [
                "support.type.property-name",
                "support.variable",
                "variable"
            ],
            "settings": {
                "foreground": "#bb9065"
            }
        },
        {
            "scope": [
                "markup.inline.raw",
                "support.constant",
                "support.class"
            ],
            "settings": {
                "foreground": "#4b76b4"
            }
        },
        {
            "name": "Storage Types",
            "scope": [
                "storage.type",
                "support.type",
                "variable.language.this"
            ],
            "settings": {
                "foreground": "#a95317"
            }
        },
        {
            "name": "Constant",
            "scope": [
                "constant",
                "keyword.other.unit"
            ],
            "settings": {
                "foreground": "#e08700"
            }
        },
        {
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#6b7610",
                "background": "#212500"
            }
        },
        {
            "name": "Strings and Characters",
            "scope": [
                "string",
                "punctuation.definition.string",
                "constant.character.escape",
                "constant.character",
                "markup.quote",
                "markup.bold",
                "markup.italic"
            ],
            "settings": {
                "foreground": "#804563"
            }
        },
        {
            "name": "Embedded Source",
            "scope": [
                "string source",
                "text source"
            ],
            "settings": {
                "background": "#000000"
            }
        },
        {
            "name": "Numeric Constants",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#bb9065"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "entity.name.type",
                "entity.other.inherited-class",
                "markup.heading.setext",
                "support.function"
            ],
            "settings": {
                "foreground": "#801050"
            }
        },
        {
        "name": "Functions Attributes",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#805590"
            }
        },
        {
            "scope": [
                "entity.name.class"
            ],
            "settings": {
                "foreground": "#bb9065"
            }
        },
        {
            "scope": [
                "entity.name.tag",
                "storage"
            ],
            "settings": {
                "foreground": "#904e5d"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable.language"
            ],
            "settings": {
                "foreground": "#e08700"
            }
        },
        {
            "name": "Control Keywords",
            "scope": [
                "entity.name.section",
                "keyword.control",                
                "constant.language",
                "punctuation.definition.heading",
                "punctuation.definition.keyword"
            ],
            "settings": {
                "foreground": "#cb4b16"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "punctuation.separator.delimiter",
                "punctuation.terminator.expression",
                "punctuation.section.array.begin",
                "punctuation.section.array.end",
                "keyword.operator",
                "storage.modifier"
            ],
            "settings":{
                "foreground": "#aa8069"
            }
        },
        {
            "scope": [
                "keyword.operator.logical"
            ],
            "settings":{
                "foreground": "#bb9065"
            }
        },
        {
            "scope": [
                "punctuation.definition.array.begin.bracket.round",
                "punctuation.definition.array.end.bracket.round",
                "punctuation.definition.arguments.begin.bracket.round",
                "punctuation.definition.arguments.end.bracket.round"
            ],
            "settings":{
                "foreground": "#bb9065"
            }
        },
        {
            "name": "Bold",
            "scope": [
                "markup.bold.markdown",
                "punctuation.definition.bold.markdown"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Italic",
            "scope": [
                "comment",
                "markup.italic.markdown",
                "punctuation.definition.italic.markdown"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "meta.brace",
                "punctuation.definition.array",
                "punctuation.definition.binding-pattern",
                "punctuation.definition.block",
                "punctuation.definition.dictionary",
                "punctuation.definition.tag",
                "punctuation.section",
                "punctuation.separator",
                "punctuation.support",
                "punctuation.terminator"
            ],
            "settings": {
                "foreground": "#bb9065"
            }
        },
        {
            "scope": "token.info-token",
            "settings": {
                "foreground": "#6796e6"
            }
        },
        {
            "scope": "token.warn-token",
            "settings": {
                "foreground": "#cd9731"
            }
        },
        {
            "scope": "token.error-token",
            "settings": {
                "foreground": "#f44747"
            }
        },
        {
            "scope": "token.debug-token",
            "settings": {
                "foreground": "#b267e6"
            }
        },
        {
            "scope": [
                "keyword.control.exception"
            ],
            "settings":{
                "foreground": "#E4B227"
            }
        },
        {
            "scope": [
                "keyword.other.new"
            ],
            "settings":{
                "foreground": "#bb9065"
            }
        },
        {
            "scope": [
                "entity.name.function.preprocessor.c",
                "variable.parameter.preprocessor.c",
                "meta.preprocessor.macro.c",
                "meta.preprocessor.diagnostic.c",
                "meta.preprocessor.include.c",
                "meta.preprocessor.c",
                "meta.preprocessor.pragma.c",
                "entity.other.attribute-name.pragma.preprocessor.c"
            ],
            "settings":{
                "foreground": "#98c379"
            }
        },
        {
            "scope": [
                "comment.block.preprocessor.if-branch.c",
                "comment.block.preprocessor.if-branch.in-block.c",
                "comment.block.preprocessor.elif-branch.c",
                "comment.block.preprocessor.elif-branch.in-block.c",
                "comment.block.preprocessor.else-branch.c",
                "comment.block.preprocessor.else-branch.in-block.c"
            ],
            "settings":{
                "foreground": "#5c6370"
            }
        }
    ]
}
